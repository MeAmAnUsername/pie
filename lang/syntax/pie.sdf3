module pie

imports
  
  lex/literal
  lex/layout
  lex/comment
  type
  data
  func_def
  func_body

context-free start-symbols
  
  Start

context-free syntax

  Start.Program =
  <<Module>
  <Package>
  
  <{Def "\n"}*>>
  
  Module.Module = <module <{ID "."}+>;>
  
  Package.Package = <package <{ID "."}+>;>
  
  Def = DataDef
  Def = FuncDef
