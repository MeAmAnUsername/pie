  prettyprint-metaborg-java(|sort):
    t -> <prettyprint-metaborg-java-TypeArguments>
    where <?"TypeArguments"> sort

  prettyprint-metaborg-java(|sort):
    t -> <prettyprint-metaborg-java-TypeArgument>
