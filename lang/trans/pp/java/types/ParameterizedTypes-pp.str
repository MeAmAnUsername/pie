    where <?"WildCard"> sort

  prettyprint-metaborg-java-TypeArguments :
    TypeArguments(t1__) -> [ H(
                               [SOpt(HS(), "0")]
                             , t1__'
                             )
                           ]
    with t1__' := <clean-layout-empty-symbols(
                   | ""
                   , FALSE()
                   , FALSE()
                   )
                   ; flatten-list> [ S("<")
                                   , <check-nonterminal-isEmpty ; id
                                      <+ pp-H-list(prettyprint-metaborg-java-TypeArgument|", ")
                                      <+ pp-one-Z(prettyprint-completion-aux)> t1__
                                   , S(">")
                                   ]

  is-metaborg-java-TypeArguments =
    ?TypeArguments(_)

  metaborg-java-get-indent :
    (t@TypeArguments(t1__), index) -> indent
    where sort := <ia-get-sort> t
          ; <?"TypeArguments"> sort
    with switch !index
           case ?1 :
             indent := ""
           otherwise :
             indent := ""
         end

  metaborg-java-get-separator :
    (t@TypeArguments(t1__), index) -> separator
    where sort := <ia-get-sort> t
          ; <?"TypeArguments"> sort
    with switch !index
           case ?1 :
             separator := ", "
           otherwise :
             separator := ""
         end

  metaborg-java-get-leading-ws :
    (t@TypeArguments(t1__), index) -> ws
    where sort := <ia-get-sort> t
          ; <?"TypeArguments"> sort
    with switch !index
           case ?1 :
