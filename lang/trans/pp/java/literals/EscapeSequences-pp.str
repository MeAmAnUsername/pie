  prettyprint-metaborg-java-OctalEscape =
    ![S(<is-string>)]

  prettyprint-metaborg-java-LastOcta =
    ![S(<is-string>)]

  prettyprint-metaborg-java-EscapeSeq :
    amb([h|hs]) -> <prettyprint-metaborg-java-EscapeSeq> h

  prettyprint-metaborg-java-OctalEscape :
    amb([h|hs]) -> <prettyprint-metaborg-java-OctalEscape> h

