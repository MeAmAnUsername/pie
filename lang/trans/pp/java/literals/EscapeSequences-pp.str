  libspoofax/term/annotation
  libstratego-sglr
  libstratego-sglr
  signatures/java/literals/EscapeSequences-sig


strategies
  prettyprint-example =
    prettyprint-metaborg-java

  prettyprint-metaborg-java =
    fail

  prettyprint-metaborg-java(|sort) =
    fail


strategies
  prettyprint-SDF-start-symbols =
    prettyprint-metaborg-java-start-symbols

  prettyprint-metaborg-java-start-symbols =
    fail


strategies
  prettyprint-metaborg-java =
    prettyprint-metaborg-java-EscapeSeq

  prettyprint-metaborg-java =
    prettyprint-metaborg-java-OctalEscape

  prettyprint-metaborg-java =
    prettyprint-metaborg-java-LastOcta

  prettyprint-metaborg-java(|sort):
    t -> <prettyprint-metaborg-java-EscapeSeq>
    where <?"EscapeSeq"> sort

  prettyprint-metaborg-java(|sort):
