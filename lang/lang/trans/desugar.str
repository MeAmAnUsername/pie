module desugar

imports

  src-gen/signatures/-

rules

  desugar-all = topdown(try(desugar))
  resugar-all = topdown(try(resugar))

  desugar: NoGenericParams() -> GenericParams([])
  resugar: GenericParams([]) -> NoGenericParams()

  desugar: NoTypeArgs() -> TypeArgs([])
  resugar: TypeArgs([]) -> NoTypeArgs()
